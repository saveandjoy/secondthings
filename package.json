{
  "name": "secondthings",
  "description": "Material library for PWA applications.",
  "author": "Olli Somero",
  "keywords": ["react", "typescript", "pwa", "components"],
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/saveandjoy/secondthings",
    "directory": "/dist"
  },
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.19",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.0",
